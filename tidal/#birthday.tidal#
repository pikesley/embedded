

d1 $ jux (rev . iter 4) $ chop 8 $ every 4 (density 1.5) $ s "claus*8" # n (irand 8)
  # size "0.5"
  # room "0.5"
  # orbit "2"
  # cutoff "1000"
  
d2 $ sound "drum ~ ~ bd*2"
  # gain "1.1"
  # orbit "2"

d3 $ every 4 (slowspread (chop) [16,32]) $ sound "~ sn:2"


d5 $ jux rev $ iter 4 $ slow 2 $ chop 8 $ sound "arc"
  # gain "1"

d4 $ jux rev $ slow 4 $ n (offadd 0.125 12 $ offadd 0.25 7 "f3(3,8) a3*3 c3(5,8) e3*2") # s "xx"
  # cutoff (slow 4 $ scale 1000 8000 $ sine1)
  # resonance "0.2"
  # gain "1.1"

d5 $ jux (iter 4) $ slowspread ($) [id,rev,density 2, iter 4] $ chop 16 $ sound "[drum future]/2([3 5]/2,8)"
  # gain "1.1"
  # crush "7"

d6 $ sound "~ wobble"
  # shape "0.2"
  # gain "1.2

d7 $ brak $ sound "feel feel:2"

