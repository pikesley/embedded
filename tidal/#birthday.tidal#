

d1 $ jux (rev . iter 4) $ chop 8 $ every 4 (density 1.5) $ s "claus*8" # n (irand 8)
  # size "0.5"
  # room "0.5"
  # orbit "2"
  # cutoff "1000"
  
d2 $ sound "drum ~ ~ bd*2"
  # gain "1.1"
  # orbit "2"

d3 $ every 4 (slowspread (chop) [16,32]) $ sound "~ sn:2"


d5 $ jux rev $ iter 4 $ slow 2 $ chop 8 $ sound "arc"
  # gain "1"

d4 $ slow 4 $ n ("f3(3,8) a3 c3 e3") # s "xx"
