d1 $ jux rev $ slow 4 $ n (offadd (1/16) 12 $ offadd 0.125 7 "c3(3,8) d3*4 e4(5,8,2) g3*2") # s "xx"
  # cutoff (scale 3000 8000 sine1)
  # resonance "0.3"
  # vowel "o"
  # speed "0.5"

d2 $ jux (iter 4) $ every 4 (rev) $ slowspread (loopAt) [2,2,8,4] $ chop 12 $ sound "shakedown"

d3 $ every 2 (chop 8) $ jux (iter 4) $ s "drum:2 [cp8 arpy*2]" # n (slow 4 $ run 8)
  # gain "1.1"
  # room "0.5"
  # size "0.5"
  # cutoff (scale 3000 8000 sine1)

d4 $ brak $ jux (2 <~) $ foldEvery [3,5] (0.25 <~) $ sound (samples "claus*8" (slow 8 $ scan 8))
  # speed "[2 1]/2"
  # room "0.5"
  # size "0.7"
  
d5 $ jux (iter 4) $ loopAt 2 $ sound "arc"
  # gain "1.2"


d6 $ degradeBy 0.75 $ jux rev $ n (offadd 0.125 12 $ offadd 0.25 7 $ slow 4 $ "f3(3,8) a3*4 c3*3 e3(3,8)]") # s "xx"
  # gain "1.1"
  # cutoff "3000"

d1 $ jux (rev . iter 4) $ chop 8 $ (s "[bd*2 [bd drum:1]]" # n (irand 4))
  # speed (scale 1 2 sine1)

d2 $ superiiter 4 $ jux (rev . chop 128) $ sound "gabba [cp wobble cp]"
  # shape "0.4"
  # speed "2"

cps 1.1