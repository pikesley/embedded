cps 1.1

d1 $ jux (iter 4) $ slowspread ($) [id, rev, density 2, (0.25 <~)] $ n "[0 [1 2] [5 ~ 4] 3]/2" # s "claus" # speed "0.5"

d2 $ jux (iter 8) $ rev $ slowspread ($) [id, rev, density 3, (0.5 <~)] $ n "[0 [1 2] [5 ~ 4] 3]/2" # s "claus" # speed "4"

d3 $ within (0,0.5) $ sound "odx(3,8)"
  # gain "1.1"


let rise = "c3 d3 e3 f3" in
d4 $ stack [sometimes (|+| n "12") $ jux rev $ n (slowspread (offadd 0.5) [7,7,12,-5] $ slow 4 $ struct "x(3,8) x x*3 x" rise) # s "dirty"
            # crush "7"
            # vowel "a e i"
            -- , sometimes (|+| n "12") $ n (slowspread (offadd 0.5) [7,7,12,-5] $ slow 4 $ struct "x x*3 x x(3,8)" rise) # s "xx" # gain "0.8"
           ]
