cps 1.1

d1 $ jux (iter 4) $ slowspread ($) [id, rev, density 2, (0.25 <~)] $ n "[0 [1 2] [5 ~ 4] 3]/2" # s "claus" # speed "0.5"

d2 $ jux (iter 8) $ rev $ slowspread ($) [id, rev, density 3, (0.5 <~)] $ n "[0 [1 2] [5 ~ 4] 3]/2" # s "claus" # speed "4"

d3 $ within (0,0.5) $ sound "odx(3,8)"
  # gain "1.1"


d4 $ sometimes (|+| n "12") $ jux rev $ n (slowspread (offadd 0.5) [7,7,12,-5] $ slow 4 $ "c3(3,8) d3 a2*3 e3") # s "dirty"
  # crush "7"
  # vowel "a e i"

d5 $ jux rev $ n (offadd (-0.125) (-12) $ slowspread (offadd 0.75) [7,7,12,7] $ slow 4 $ "c3 d4 a3 e3") # s "xx"
  # gain "1.2"
  
  # cutoff "3000"
  # resonance "0.7"
