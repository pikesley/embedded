
solo $ 
d1 $ (slow 4 $ n (slowspread (offadd 0.125) [7,-5] $ "e3(3,8) f3(5,8,[2 4 6]/3)") # s "xx")
  # room (slow 4 sine1)
  # size "0.5"

d2 $ slow 2 $ (jux (inside (1/8) rev) $ s (unwrap $ e <$> ((+1) <$> (slow 8 $ run 8)) <*> "[8]/16" <*> (pure "cp")))
  # vowel "a e i o u"

d3 $ slow 2 $ struct "x ~ x [x x*2]/2" $ s "drum"

d1 $ often (off 1 (|*| speed "1.5")) $ sometimes (off 0.125 (# up "[5 -7 12]/6")) $ struct "x [x x]" $ n ("[[c3,e4] d3 ds3 f3]/8") # s "xx"
  # room "1"
  # size "0.9"
  # orbit "1"
  # crush "3"
  
cps 0.9

d4 $ off 0.125 (|*| up "[7 -5 12]/2") $ n ("[c d ds [a e]]/8") # s "ravebass"
  # gain "1.5"
  # orbit "3"
  # speed "2"
  # room "1"
  # size "0.9"

d2 $ jux (iter 4)  $ whenmod 4 2 rev $ every 2 (chop 8) $ sound "glitch:2(3,8)"
  # gain "1.9"
  # orbit "2"
  # vowel "i"

d3 $ iter 8 $ slow 2 $ chop 16 $ sound "arc"
  # unit "c" # speed "0.5"
  # gain "1.3"
