

d1 $ (slow 4 $ n (slowspread (offadd 0.125) [7,-5] $ "e3(3,8) f3(5,8,[2 4 6]/3)") # s "xx")
  # room (slow 4 sine1)
  # size "0.5"

d2 $ (jux (inside (1/8) rev) $ s (unwrap $ e <$> ((+1) <$> (slow 8 $ run 8)) <*> "[8]/16" <*> (pure "cp")))
  # vowel "a e i o u"

d3 $ slow 2 $ struct "x ~ x [x x*2]/2" $ s "drum"

d4 $ rarely (off 0.125 (# up "[5 7]/4")) $ struct "x ~ [x ~ x] " $ n ("[[c4,e4] [d4,g4] [ds4,bs4] [f4,es3,a4]]/8") # s "xx"