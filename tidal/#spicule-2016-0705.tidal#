
x1 $ slow 2 $ jux (rev . chop 8) $ slow 4 $ sound "future:3*3 ~ future:6*2 ~"
  # speed "4"
  # room "0.8"
  # size "0.8"
  # orbit "1"

x2 $ slow 2 $ jux (1 <~) $ slowspread (slow) [1,1.5,3] $ off 0.125 (|*| speed "2") $ sound "sd8:3 [sd8 sd8:5(3,8,[0 4 2]/3)]/2"
  # speed "2"  # orbit "0"
  
x3 $ slow 2 $ chop 8 $ s "[lt8*2 lt8*4]/2 mt8*3"
  # crush "5" # speed "4"

x4 $ jux rev $ slow 4 $ off (1/4) (|*| speed (scale 0.5 1 sine1)) $ ((chop 16 $ sound "arc")
  # speed (scale 1.5 2 sine1))


cps 1.2

d1 $ stack [stut 2 0.4 (-0.125) $ runWith 4 (density 4) $ sound "claus*2" # pan "0" # speed "2"
            # cutoff "1000"
           ,
            stut 2 0.4 (-0.125) $ runWith 4 (density 2) $ slow 2 $ runWith 4 (density 2) $ sound "claus:5*3" # pan "1"
            # cutoff "3000"
           ]

d2 $ jux (rev) $ slow 2 $ ghost $ sound "drum [feel:1 ~ ~ mt8:5] [~ drum] [~ [voodoo ~ voodoo:4 mt8:3]]" # resonance (scale 0.4 0.5 tri1) # cutoff "3000" 

d3 $ jux (0.5 <~) $ slowspread (chop) [2,3] $ sound "arpy:5 arpy"
  # resonance (scale 0.4 0.5 tri1) # cutoff (slow 4 $ scale 3000 1500 tri)
  # speed "[1,1.5]"
  # crush "2"
  # accelerate "[0 -2]/4"

d4 $ sound "~ [~ [ind:7 ~ ind:7]]"

d5 $ jux rev $ runWith 2 (density 2) $ iter 4 $ loopAt 8 $ chop 32 $ sound "shakedown"




  
d3 $ slow 2 $ n (slow 4 $ offadd 0.25 12 $ offadd 0.125 5 $ slow 2 $ "c6(3,8) d7*3 c7*3 g7(3,8)") # s "xx" 
  # resonance "0.4"
  # cutoff (slow 2 $ scale 3000 1000 sine1)
  # end "0.4"
  # pan "1"

x4 $ rev $ slow 2 $ n (slow 4 $ offadd 0.125 12 $ offadd 0.25 5 $ slow 2 $ "c6(3,8) d7*3 c7*3 g7(3,8)") # s "xx" 
  # resonance "0.4"
