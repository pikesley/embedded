cps 0.715

d1 $ runWith 4 (density 2) $ jux rev $ n "[0 4] [[2 1] 3 [2 4] 1]@3" # s "voodoo"
  # gain "1.1"

d2 $ sound "~ [cp ~ cp]"
  # speed "2"

d3 $ (n (offadd 0.125 7 $ offadd 0.25 12 $ slow 4 $ "f0(3,8) a0*2 g0(5,8) d0*3") # s "xx")
  # cutoff (slow 4 $ scale 1000 3000 sine1)
  # resonance "0.35"

d2 $ stack [jux rev $ (n $ offadd 0.125 12 $ slowspread (offadd 0.25) [7,12] $ slow 4 $ "f0(3,8) [~ a0*2] g0(3,8) d0") # s "xx" # gain "0.95"
           ]

d4 $ runWith 4 (density 2) $ loopAt 4 $ chop 64 $ sound "[shackup]"

d6 $ slow 4 $ sometimes (# up "7 12") $ iter 4 $ chop 16 $ sound "fm:10"
  # gain "0.9"

(d5 $ (loopAt 4 $ chop 16 $ sound "paper"
       # unit "c"
       # gain "0.8"))

d1 $ sound "gab(3,8)"
