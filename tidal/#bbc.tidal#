cps 0.715

d1 $ jux (iter 4) $ sound "[claus ~ claus:4] claus:3*2@3"

d2 $ every 3 (|*| speed "1.3") $ jux (iter 4) $ loopAt 4 $ chop 16 $ sound "shackup"
  # crush "2"

d3 $ stack [jux rev $ (n $ slow 4 $ "f0 a0 g0 d0") # s "xx" # gain "0.95"
           ]

d4 $ loopAt 4 $ chop 64 $ sound "shackup"

d5 $ slow 4 $ chop 16 $ sound "fm:10"
  # gain "0.9"

(d6 $ (loopAt 4 $ chop 16 $ sound "paper"
       # unit "c"
       # gain "0.8"))

d1 $ jux rev $ n (offadd 0.125 12 $ offadd 0.25 7 $ slow 4 $ "f(3,8) a(2,8) c(3,8) e(3,8)") # s "superchip"
  # sustain "0.2"
  # gain "0.9"


d1 $ jux rev $ n (offadd 0.25 12 $ offadd 0.125 7 $ slow 4 "[f0(3,8) g0(3,8)]/2 a0(3,8) c0(3,8) e0*3") # s "xx"
  # resonance "0.35"
  # cutoff (scale 1000 3000 $ slow 4 sine1)
  # gain "0.9"

d2 $ jux (iter 4) $ loopAt 2 $ chop 32 $ sound "[os os:1]/2" # speed "-1"

d3 $ jux (# speed "1.5")$  slow 4 $ iter 4 $ chop 8 $ sound "fm:10"
  # gain "0.9"

  