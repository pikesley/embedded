cps 0.5

d1 $ slowspread (\x -> (inside x (juxBy 0.5 rev))) [1,4,8,2] $ (density 2 . (# speed "2")) $ sound "bd sn [sn:2 sn:5]" # pan "0"

d2 $ jux' [id,rev,rev . (# speed "0.5"), (|*| up "12")] $ n (offadd 0.125 7 (slow 2 "[c3(3,8) d4] e4([3 5]/2,[8 4]/2) f3*3 a4*4")) # s "xx"

d1 $ n "0 0 0 0" #s "wobble" # pan saw1