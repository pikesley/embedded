d1 $ slow 2 $ substruct "x [~ x/2]" $ slowspread (chop) [8,4,2] $ s "lt8 mt8 ~ ht8" # n (irand 8)
  

d2 $ brak $ jux (iter 4) $ chop 2 $ sound "feel feel:2"

