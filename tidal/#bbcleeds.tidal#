cps 0.715


d1 $ jux (rev) $ runWith 4 (density 2) $ slow 2 $ n "0 1 [2 1] [3 ~ 3] 0 [[~ 1] 2 4 4]@3"  # s "voodoo"
  # gain "1.2"

d2 $ sound "drum [~ cp:2]"
  # gain "1"

-- g0 d0
d3 $ n (offadd 0.125 12 $ offadd 0.25 7 $ slow 2 $ "f0(3,8) a0*2") # s "xx"
  # cutoff (slow 4 $ scale 1000 3000 sine1)
  # resonance "0.3"

d4 $ loopAt 4 $ chop 64 $ sound "[shackup]"

d5 $ slow 4 $ chop 16 $ sound "fm:10"
  # gain "0.9"

(d6 $ (loopAt 4 $ chop 16 $ sound "paper"
       # unit "c"
       # gain "0.8"))

d7 $ sometimes (rev . chop 8) $ sometimes (|+| speed "2") $
  jux (0.25 <~) $ sound (samples "[ifdrums*8, future*8]" (run 8))
  # nudge "[0 0.04]*4" # shape "0.2"
  # gain "0.9"
