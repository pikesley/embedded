x1 $ jux rev $ brak $ chop 2 $ slow 4 $ sound "bd8:4(3,8) cp*4"
  # speed "2"

d2 $ jux rev $ slow 8 $ n (offadd (1/16) 12 $ offadd 0.125 (-7) $ (+60) <$> (palindrome "c3(3,8) a3(3,8) f3(3,16) e3*4")) # s "xx"
  # gain "0.9"
  # resonance "0.3"
  # cutoff (scale 2000 4000 tri1)

d3 $ jux rev $ loopAt 8 $ (chop 32 $ s "[sometimes ~ ~]/3")
  # vowel "[a e]*4"

x1 $ (jux rev $ slowspread (chop) [64,128] $ sound "drum cp*2? [arpy:4 arpy:5]/2 drum?")
  # nudge (slow 1 $ sine1)
  # crush "3"
