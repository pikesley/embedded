d1 $ sometimes (# speed "1.2") $ slow 2 $ runWith 4 (rev . density 2 . chop 16) $ sound "drum [ht8 [ht8 mt8]] lt8 [~ ht8]"
  # room "0.5"
  # size "0.4"
  # orbit "1"

d2 $ slow 2 $ 0.25 ~> (palindrome $ chop 32 $ sound "~ cp")

d3 $ jux rev $ loopAt 2 $ every 4 (slow 1.5) $ chop 8 $ s "os" # n (choose [0,1,2])

hush

d1 $ jux rev $ (slowspread (chop) [32,64] $ sound "bd arpy sn:1 claus")
  # cutoff (slow 4 $ scale 1000 3000 sine1)
  # resonance "0.2"

d2 $ iter 4 $ slowspread (chop) [8,6,3] $ sound "~ off"


d3 $ degradeBy 0.8 $ jux rev $ n (sometimes ((+12) <$>) $ offadd 0.125 12 $ offadd 0.25 7 $ slow 8 $ "[c0 d0]/2(3,8) a0*3 f0(3,8,[2 4]/2) e0*2") # s "xx"
  # resonance "0.2"
  # end "0.2"
  # cutoff (scale 1000 3000 $ slow 4 $ sine1)

d5 $ runWith 4 (slowspread (density) [2]) $ jux (iter 4) $ slowspread (loopAt) [2,4,1] $ chop 8 $ sound "[os os:1 os:2]/3"


d7 $ degradeBy 0.8 $ sometimes (# speed "2") $ foldEvery [3,5] (0.25 <~) $ jux (1 <~) $ every 2 (rev . chop 8) $ slow 2 $ brak $ sound "{ht lt [~ mt] lt(3,8), arpy [arpy:2 arpy:4]/2 arpy:7}"
  # crush "[3 4 5]/3"

d8 $ sound "bd*4 [off off*2]/2"

d9 $ jux (rev. chop 8)$ n (run 8) # s "gabba"
